{
    "name": "intbizth/admin-bundle",
    "type": "symfony-bundle",
    "description": "AdminBundle for Using SyliusGridBundle",
    "keywords": ["admin"],
    "homepage": "http://intbizth.com",
    "license": "INTBIZTH",
    "authors": [
        {
            "name": "Ishmael",
            "email": "phaiboon@intbizth.com",
            "homepage": "http://intbizth.com"
        }
    ],
    "require": {
        "php": "^5.5.9|^7.0",

        "jms/serializer-bundle": "^0.13|^1.0",
        "knplabs/knp-menu-bundle": "~1.1",
        "liip/imagine-bundle": "~1.0",
        "webmozart/assert": "^1.0",
        "white-october/pagerfanta-bundle": "~1.0",
        "willdurand/hateoas-bundle": "^0.4|^1.0",
        "winzou/state-machine-bundle": "~0.2",
        "zendframework/zend-hydrator": "^1.0|^2.0",
        "zendframework/zend-stdlib": "^2.0|^3.0",

        "sylius/registry": "0.19.*",
        "sylius/storage": "0.19.*",
        "sylius/resource": "0.19.*",
        "sylius/resource-bundle": "0.19.*",
        "sylius/grid": "0.19.*",
        "sylius/grid-bundle": "0.19.*",
        "sylius/ui-bundle": "0.19.*",
        "sylius/rbac": "0.19.*",
        "sylius/rbac-bundle": "0.19.*",
        "sylius/user": "0.19.*",
        "sylius/user-bundle": "0.19.*",
        "sylius/mailer": "0.19.*",
        "sylius/mailer-bundle": "0.19.*",
        "sylius/money-bundle": "0.19.*",
        "sylius/currency": "0.19.*",
        "sylius/currency-bundle": "0.19.*",
        "sylius/locale": "0.19.x-dev",
        "sylius/locale-bundle": "0.19.x-dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Intbizth\\Bundle\\AdminBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Intbizth\\Bundle\\AdminBundle\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.19-dev"
        }
    }
}
